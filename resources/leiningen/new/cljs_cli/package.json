{
  "name": "<%name%>",
  "version": "0.1.0",
  "description": "FIXME",
  "main": "target/main.js",
  "author": "FIXME <FIXME@FIXME.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/FIXME/<%raw-name%>"
  },
  "directories": {
    "bin": "bin",
    "doc": "docs",
    "lib": "lib",
    "src": "src"
  },
  "bin": {
    "<%name%>": "bin/<%name%>"
  }<%#figwheel-main?%>,
  "scripts": {
    "build": "lein fig:prod",
    "init": "lein fig:build",
    "repl": "lein repl",
    "start": "lein fig:build",
    "test": "lein fig:test"
  }<%/figwheel-main?%><%#lein-figwheel?%>,
  "scripts": {
    "build": "lein cljsbuild once prod",
    "init": "lein figwheel",
    "repl": "lein repl",
    "start": "lein figwheel",
    "test": "lein cljsbuild once test && node target/test.js"
  }<%/lein-figwheel?%><%#shadow?%>,
  "scripts": {
    "build": "./scripts/build",
    "init": "npm install && npx shadow-cljs watch app",
    "repl": "npx shadow-cljs cljs-repl app",
    "test": "npx shadow-cljs compile test"
  },
  "dependencies": {<%#cljs-deps.common%>
    "<%name%>": "^<%version%>"<%^last%>,<%/last%><%/cljs-deps.common%>
  },
  "devDependencies": {<%#cljs-deps.dev%>
    "<%name%>": "^<%version%>",<%/cljs-deps.dev%><%#cljs-deps.shadow%>
    "<%name%>": "^<%version%>"<%^last%>,<%/last%><%/cljs-deps.shadow%>
  }<%/shadow?%>
}
